public class Sum {
	static int sum(int[] array)
	{
		int sum = 0;
		for(int i =0; i<array.length; i++)
		{
			if(array[i]>0)
			{
				sum += array[i];
			}
		}
		return sum;
	}
public static void main(String[] args) {
	System.out.println(sum(new int[]{10,4,5,6,7})); 
	System.out.println(sum(new int[]{-10,-4,-5,-6,-7}));
}
}
